{
	"typescript.tsc.autoDetect": "off",
	"jake.autoDetect": "off",
	"grunt.autoDetect": "off",
	"npm.autoDetect": "off",
	"gulp.autoDetect": "off",
	"files.exclude": {
		"**/.git": true,
		"**/.vscode": false,
		"**/Intermediate": true,
		"**/Saved": true
	},
	"files.associations": {
		"chrono": "cpp",
		"*.rh": "cpp",
		"resumable": "cpp",
		"mutex": "cpp",
		"future": "cpp",
		"system_error": "cpp"
	},
	"editor.insertSpaces": false,
	"editor.detectIndentation": false,
	"search.useIgnoreFiles": false,
	"C_Cpp.intelliSenseEngine": "Tag Parser",
}
